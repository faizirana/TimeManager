# PostgreSQL settings (not needed for tests - tests use SQLite in-memory)
POSTGRES_USER=myuser
POSTGRES_PASSWORD=mypass
POSTGRES_DB=mydb
POSTGRES_HOST=db

# JWT Secrets - REQUIRED for authentication to work
# Generate secure secrets using: openssl rand -base64 32
ACCESS_TOKEN_SECRET=your_access_token_secret_here_min_32_characters
REFRESH_TOKEN_SECRET=your_refresh_token_secret_here_min_32_characters
