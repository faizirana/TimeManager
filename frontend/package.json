    "@types/d3-path": "^3.1.1",
    "@types/d3-scale": "^4.0.9",
    "@types/d3-shape": "^3.1.7",
    "@types/d3-time": "^3.0.4",
    "@types/d3-timer": "^3.0.2",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "d3-array": "^3.2.4",
    "d3-color": "^3.1.0",
    "d3-ease": "^3.0.1",
    "d3-format": "^3.1.0",
    "d3-interpolate": "^3.0.1",
    "d3-path": "^3.1.0",
    "d3-scale": "^4.0.2",
    "d3-shape": "^3.2.0",
    "d3-time": "^3.1.0",
    "d3-time-format": "^4.1.0",
    "d3-timer": "^3.0.1",
    "decimal.js-light": "^2.5.1",
    "dom-helpers": "^5.2.1",
    "eventemitter3": "^4.0.7",
    "fast-equals": "^5.4.0",
    "internmap": "^2.0.3",
    "jose": "^6.1.3",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.562.0",
    "next": "15.5.4",
    "next-themes": "^0.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-is": "^18.3.1",
    "react-smooth": "^4.0.4",
    "react-transition-group": "^4.4.5",
    "recharts": "^2.15.4",
    "recharts-scale": "^0.4.5",
    "tiny-invariant": "^1.3.3",
    "victory-vendor": "^36.9.2"
